{
  "name": "backend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon -e ts,js,yaml -w src -x babel-node --inspect src/index.ts --extensions .ts,.js",
    "start": "run-p start:dev watch ",
    "build": "cross-env NODE_ENV=production rimraf build && tsc --noEmit && babel src -d build -s --extensions .js,.ts",
    "serve": "cross-env NODE_ENV=production node build/index.js",
    "clean": "rimraf build dist",
    "watch": "tsc --noEmit -w",
    "generate-types": "rimraf src/api-v1 && openapi-generator generate -c openapi-config.json -i src/v1-openapi.yaml -g typescript-fetch -o src/api-v1 && chmod +x postgenerate.sh && ./postgenerate.sh",
    "lint": "eslint --ext .js,.ts src/"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "swagger-ui-express": "^4.1.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/yamljs": "^0.2.30",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-putout": "^3.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-exports": "git+https://github.com/andres-gr/eslint-plugin-sort-exports.git",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
